        "ext-pdo_sqlite": "*",
        "mockery/mockery": "0.7.2",
        "phpunit/phpunit": "3.7.*",
        "hamcrest/hamcrest-php": "1.1.1",
        "doctrine/couchdb": "@dev"
    },
    "authors": [
        {
            "name": "Maxim Gnatenko",
            "email": "maxim@xiag.ch"
        },
        {
            "name": "Anton Bondar",
            "email": "zebooka@xiag.ch"
        }
    ],
    "autoload": {
        "psr-0": {
            "Magomogo\\Persisted": ["lib/"],
            "Magomogo\\Persisted\\Test": ["test/_classes"]
        }
    }
